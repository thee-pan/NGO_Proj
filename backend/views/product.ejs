<%-include ('partials/header')%>


        <section style="background-color: #eee;">
            <div class="container py-5">
        
        
                <div class="row">
                    <div class="col-lg-4">
                        <div class="card mb-4">
                            <div class="card-body text-center">
        
                                <h2 class="my-3">
                                    <%=product.name%>
                                </h2>
                                <h5 class="text-muted mb-1">
                                    <%=product.purpose%>
                                </h5>
                                <h5 class="text-muted mb-4">
                                    <%=product.address%>
                                </h5>
                                <div class="d-flex justify-content-center mb-2">
                                    <button type="button" class="btn btn-dark">Support</button>
                                    <button type="button" class="btn btn-outline-dark ms-1">Message</button>
                                </div>
                            </div>
                        </div>
                        <div class="card mb-4 mb-lg-0">
                            <div class="card-body p-0">
                                <ul class="list-group list-group-flush rounded-3">
                                    <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                                        <i class="fas fa-globe fa-lg text-warning"></i>
                                        <p class="mb-0">
                                            <%=product.website%>
                                        </p>
                                    </li>
                                    <li class="list-group-item d-flex justify-content-between align-items-center p-3">
                                        <i class="fab fa-github fa-lg" style="color: #333333;"></i>
                                        <p class="mb-0">
                                            <%=product.email%>
                                        </p>
                                    </li>
        
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="card mb-4">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Objective</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0">
                                            <%=product.objectives%>
                                        </p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Person of Contact</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0">
                                            <%=product.poc%>
                                        </p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Phone</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0">
                                            <%=product.phone%>
                                        </p>
                                    </div>
                                </div>
                                <hr>
                                <div class="row">
                                    <div class="col-sm-3">
                                        <p class="mb-0">Mobile</p>
                                    </div>
                                    <div class="col-sm-9">
                                        <p class="text-muted mb-0">
                                            <%=product.mobile%>
                                        </p>
                                    </div>
                                </div>
                                <hr>
        
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
       

    </body>

</html>